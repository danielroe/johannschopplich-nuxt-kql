{
  "name": "nuxt-kql",
  "type": "module",
  "version": "0.27.0",
  "packageManager": "pnpm@8.5.1",
  "description": "Nuxt 3 module for Kirby's Query Language API",
  "author": "Johann Schopplich <pkg@johannschopplich.com>",
  "license": "MIT",
  "homepage": "https://nuxt-kql.byjohann.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/nuxt-kql.git"
  },
  "bugs": {
    "url": "https://github.com/johannschopplich/nuxt-kql/issues"
  },
  "keywords": [
    "nuxt",
    "nuxt3",
    "kirby",
    "kql",
    "query"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "require": "./dist/module.cjs",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "docs": "pnpm -C docs run dev",
    "docs:build": "pnpm -C docs run build",
    "docs:serve": "pnpm -C docs run serve",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:run": "vitest --run",
    "test:types": "tsc --noEmit",
    "release": "bumpp --commit --push --tag",
    "prepare": "nuxi prepare playground && nuxi prepare test/fixture"
  },
  "dependencies": {
    "@nuxt/kit": "^3.5.1",
    "@vueuse/core": "^10.1.2",
    "defu": "^6.1.2",
    "kirby-fest": "^0.3.3",
    "ofetch": "^1.0.1",
    "ohash": "^1.1.2",
    "scule": "^1.0.0",
    "ufo": "^1.1.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.1",
    "@nuxt/module-builder": "^0.3.1",
    "@nuxt/test-utils": "^3.5.1",
    "@types/node": "^18.16.13",
    "bumpp": "^9.1.0",
    "destr": "^1.2.2",
    "eslint": "^8.41.0",
    "nuxt": "^3.5.1",
    "nuxt-kql": "workspace:*",
    "typescript": "^5.0.4",
    "vitest": "^0.31.1",
    "vue-tsc": "^1.6.5"
  }
}
